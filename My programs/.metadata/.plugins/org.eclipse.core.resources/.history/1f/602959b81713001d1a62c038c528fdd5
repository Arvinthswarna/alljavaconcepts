package inheritance;

class Company {
	String company_name = "Axis";
	
}
class Employee extends Company{
	int emp_id;
	String emp_name;
	
	Employee(int emp_id,String emp_name){
	this.emp_id = emp_id;
	this.emp_name = emp_name;
}
	void get_info() {
	System.out.println(this.emp_id + " " + this.empname + " " + this.company_name );
		
	}
	
}

public class InheritDemo1 {
	public static void main(String[] args) {
		
		
		
	}

}
