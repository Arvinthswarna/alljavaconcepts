package javaarrays;

public class Sample3 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numbers[] = new int[5];
		numbers[0] = 20;
		numbers[1] = 40;
		numbers[2] = 60;
		numbers[3] = 80;
		numbers[4] = 100;
		
		for (int i = 0; i < numbers.length; i++) {
			System.out.println(numbers[i]);
		}
	}
}
